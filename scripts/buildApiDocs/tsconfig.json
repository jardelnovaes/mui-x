{
  "compilerOptions": {
    "allowJs": true,
    "isolatedModules": true,
    "noEmit": true,
    "noUnusedLocals": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "types": ["node", "mocha"],
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@mui/markdown": ["../../node_modules/@mui/monorepo/packages/markdown/index.d.ts"],
      "docs/config": ["../../node_modules/@mui/monorepo/docs/config.d.ts"],
      "@mui-internal/api-docs-builder": [
        "../../node_modules/@mui/monorepo/packages/api-docs-builder/index.ts"
      ],
      "@mui-internal/api-docs-builder/*": [
        "../../node_modules/@mui/monorepo/packages/api-docs-builder/*"
      ]
    }
  },
  "include": ["./**/*.ts"]
}
